{
  "name": "voila",
  "version": "0.1.0",
  "description": "Convert HTML to Typeform on the fly.",
  "main": "app.js",
  "dependencies": {
    "webpack": "^1.14.0"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "browser-env": "^2.0.19",
    "webpack-dev-server": "^1.16.2"
  },
  "scripts": {
    "test": "ava",
    "build": "webpack --config webpack.config.js",
    "start": "./node_modules/.bin/webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Typeform/voila.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Typeform/voila/issues"
  },
  "homepage": "https://github.com/Typeform/voila#readme",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "ava": {
    "files": [
      "src/*.test.js",
      "src/*/*.test.js"
    ],
    "require": [
      "babel-register",
      "./src/test/helpers/setup-browser-env.js"
    ],
    "babel": "inherit"
  }
}
